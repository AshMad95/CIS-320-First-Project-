<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>NameListGet</servlet-name>
        <servlet-class>edu.simpson.madsen.NameListGet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListGet</servlet-name>
        <url-pattern>/api/name_list_get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NameListEdit</servlet-name>
        <servlet-class>edu.simpson.madsen.NameListEdit</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListEdit</servlet-name>
        <url-pattern>/api/name_list_edit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NameListDelete</servlet-name>
        <servlet-class>edu.simpson.madsen.NameListDelete</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListDelete</servlet-name>
        <url-pattern>/api/name_list_delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetLogin</servlet-name>
        <servlet-class>edu.simpson.madsen.GetLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetLogin</servlet-name>
        <url-pattern>/api/get_login_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SetLogin</servlet-name>
        <servlet-class>edu.simpson.madsen.SetLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SetLogin</servlet-name>
        <url-pattern>/api/set_login_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InvalidateLoginServlet</servlet-name>
        <servlet-class>edu.simpson.madsen.InvalidateLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>InvalidateLoginServlet</servlet-name>
        <url-pattern>/api/invalidate_login_servlet</url-pattern>
    </servlet-mapping>

</web-app>